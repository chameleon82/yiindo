{
    "name": "chameleon82/yiindo",
    "description": "Yii2 CMS",
    "keywords": ["yii2"],
    "homepage": "https://github.com/chameleon82/yiindo",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/chameleon82/yiindo/issues?q=is%3Aopen",
        "source": "https://github.com/chameleon82/yiindo"
    },
    "authors": [
        {
            "name": "Alex Nekrasov",
            "email": "snake_nas@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-imagine": "*",
        "yiisoft/yii2-authclient": "~2.0.0",
        "newerton/yii2-fancybox": "dev-master",
        "kartik-v/yii2-widget-fileinput": "*",
        "kartik-v/yii2-widget-select2": "*",
        "kartik-v/yii2-slider": "dev-master",
        "yiisoft/yii2-jui": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
